{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "ecmaVersion": 6,
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "before": true,
    "inject": true,
    "Uint8Array": true
  },
  "rules": {
        "no-unused-vars": ["error", {
          "vars": "all",
          "args": "none"
        }],
        "no-console": "off"
    }
}
